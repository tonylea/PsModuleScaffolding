{
  "configurations": [
    {
      "name": "PowerShell Attach to Host Process",
      "type": "PowerShell",
      "request": "attach",
      "runspaceId": 1
    },
    {
      "name": "PowerShell Interactive Session",
      "type": "PowerShell",
      "request": "launch",
      "cwd": ""
    },
    {
      "name": "PowerShell Launch Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "cwd": "${file}"
    }
  ]
}
